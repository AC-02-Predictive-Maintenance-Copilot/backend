generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Machine {
  id   String @id @default(uuid())
  productId String
  type String
  airTemperature Float
  processTemperature Float
  rotationalSpeed Int
  torque Float
  toolWear Int
  target Int
  failureType String

  @@map("machines")
}
